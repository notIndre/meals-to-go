{
  "expo": {
    "name": "Meals To Go",
    "slug": "meals-to-go",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./meow_blep.png",
    "splash": {
      "image": "./assets/splashpig.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/meow_blep.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/meow_blep.png"
    }
  }
}
